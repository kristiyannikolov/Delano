    <div class="col-lg-7">
    <div class="panel panel-primary">
    <div class="panel-heading">
        <h2 class="panel-title">{{ td.title }}</h2> <blockquote><p class="text-right">{{ td.date }}</p></blockquote>
        </div>
<div class="panel-body">
        <p>{{ td.body }}</p>
        </div>
        </div>
        </div>
        </div>
        </div>
    {% endfor %}
{% endblock %}
